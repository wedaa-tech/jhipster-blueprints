APP_NAME=<%= baseName %>
APP_VERSION=1.0.0
APP_DESCRIPTION=<%= baseName %>
SERVER_PORT=<%= serverPort != null ? serverPort : 9001 %>

<%_ if(postgresql) { _%>
POSTGRESQL_DB_URL=postgresql://<%= baseName %>@localhost:<%= databasePort != null ? databasePort : 5432 %>/<%= baseName %>
<%_ } _%>

<% if(mongodb) { %>
MONGO_DB_URL=mongodb://localhost:<%= databasePort != null ? databasePort : 27017 %>/<%= baseName %>

<% } %>

<%_ if(eureka) { _%>
EUREKA_SERVER_URL=http://admin:admin@localhost:8761/eureka
PORT=8080
OTHER_SERVICE_NAME=<%= baseName %>
EUREKA_SERVER_INSTANCES=1
OTHER_SERVICE_URL=http://localhost:8080
<%_ } _%>



<%_ if (restServer?.length && !eureka && apiServers){ apiServers.forEach((appServer) =>  { _%>
COMMUNICATION_<%= appServer.baseName.toUpperCase() %> = http://localhost:<%= appServer.serverPort %>
<%_ })} _%>

<%_ if (rabbitmqClient?.length||rabbitmqServer?.length){  _%>
RABBIT_USER='guest'
RABBIT_PS='guest'
PIKA_HOST='localhost'
PIKA_PORT=5672
<%_ } _%>
